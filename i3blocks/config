[volume]
command=~/.local/libexec/i3blocks/volume-pulseaudio -H 墳 -M 奔 -L 奄 -X 婢 -F '$SYMB $VOL%' -f '$SYMB $VOL%'
interval=once
signal=10

[datetime]
command=~/.local/libexec/i3blocks/calendar
interval=10
label= 

